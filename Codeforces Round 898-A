#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t; cin>>t;
    while(t--){
        string s1; cin>>s1;
        string s2="abc";
        int cnt=0;
        for(int i=0; i<3; i++){
            if(s1[i]!=s2[i]){
                cnt++;
            }
        }
        if(cnt<=2)
            cout<<"Yes"<<'\n';
        else
            cout<<"No"<<'\n';
    }
    return 0;
}
