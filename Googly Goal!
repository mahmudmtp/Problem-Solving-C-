#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--){
        string s;
        cin>>s;
        int len=s.size();
        string s2=s;
        reverse(s.begin(),s.end());
        if(s2==s){
            for(int i=0; i<len; i++){
                cout<<s<<' ';
            }
            cout<<'\n';
        }
        else{
            cout<<"Fool "<<s2<<"!"<<endl;
        }
    }
    return 0;
}
