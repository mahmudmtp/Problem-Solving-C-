#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        for(int i=0; i<2*n; i++){
            for(int j=0; j<2*n; j++){
                int ans=i/2+j/2;
                if(ans%2==0)
                    cout<<"#";
                else
                    cout<<".";
            }
            cout<<'\n';
        }
    }
    return 0;
}
