#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--){
        int n=8;
        string s[n];
        for(int i=0; i<8; i++){
            cin>>s[i];
        }
        string res;
        for(int i=0; i<8; i++){
            for(int j=0; j<8; j++){
                if(s[i][j]!= '.')
                    res+=s[i][j];
            }
        }
        cout<<res<<'\n';
    }
    return 0;
}
