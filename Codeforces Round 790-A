#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t; cin>>t;
    while(t--){
        string s; cin>>s;
        int a=0;
        for(int i=0; i<s.size()/2; i++){
            a+=s[i];
        }
        int b=0;
        for(int i=s.size()/2; i<s.size(); i++){
            b+=s[i];
        }
        if(a==b)
            cout<<"Yes"<<'\n';
        else
            cout<<"No"<<'\n';
    }
    return 0;
}
