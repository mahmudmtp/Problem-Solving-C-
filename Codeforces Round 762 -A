#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t; cin>>t;
    while(t--){
        string s;
        cin>>s;
        string s1=" ",s2=" ";
        for(int i=0; i<s.size()/2; i++){
            s1+=s[i];
        }
        for(int i=s.size()/2; i<s.size(); i++){
            s2+=s[i];
        }
        if(s1==s2)
            cout<<"Yes"<<'\n';
        else
            cout<<"No"<<'\n';
    }
    return 0;
}
