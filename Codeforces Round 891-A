#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;cin>>t;
    while(t--){
        int n;cin>>n;
        int a[n];
        int res=0;
        for(int i=0; i<n; i++){
            cin>>a[i];
            res+=a[i];
        }
        if(res%2==0){
            cout<<"Yes"<<'\n';
        }
        else{
            cout<<"No"<<'\n';
        }
    }
    return 0;
}
