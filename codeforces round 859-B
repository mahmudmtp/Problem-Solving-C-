#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t; cin>>t;
    while(t--){
        int n; cin>>n;
        int a[n];
        for(int i=0; i<n; i++){
            cin>>a[i];
        }
        int even=0,odd=0;
        for(int i=0; i<n; i++){
            if(a[i]%2==0) even+=a[i];
            if(a[i]%2!=0) odd+=a[i];
        }
        if(even>odd)
            cout<<"Yes"<<'\n';
        else
            cout<<"No"<<'\n';
    }
    return 0;
}
