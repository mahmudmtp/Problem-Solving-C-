#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;cin>>t;
    while(t--){
        string s="codeforces";
        char c;
        cin>>c;
        bool ans=false;
        for(int i=0; i<s.size(); i++){
            if(s[i]==c)
                ans=true;
        }
        if(ans)
            cout<<"Yes"<<'\n';
        else
            cout<<"No"<<'\n';
    }
    return 0;
}
