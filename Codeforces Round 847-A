#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--){
        string s="314159265358979323846264338327";
        string s1;
        cin>>s1;
        int cnt=0;
        for(int i=0; i<s1.size(); i++){
            if(s[i]==s1[i])
                cnt++;
            else
                break;
        }
        cout<<cnt<<'\n';
    }
    return 0;
}
