#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t; cin>>t;
    while(t--)
    {
        int n; cin>>n;
        n*=2;
        int cnt1=0,cnt2=0;
        for(int i=0; i<n; i++){
            int x; cin>>x;
            if(x%2==0)
                cnt1++;
            else
                cnt2++;
        }
        if(cnt1==cnt2)
            cout<<"Yes"<<'\n';
        else
            cout<<"No"<<'\n';
    }
    return 0;
}
